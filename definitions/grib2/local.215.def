concept marsStream(unknown) {
    # Ensemble data assimilation system
    # ... uninitialized analysis
    enda = {
      typeOfGeneratingProcess = 0;
      backgroundProcess = 2;}
    # fg
    enda = {
      typeOfGeneratingProcess = 2;
      backgroundProcess = 0;
    }
    # ... analysis increment (to be defined)
    # ... initialized analysis (to be defined)
